{
  "name": "@manylead/emails",
  "version": "1.0.0",
  "description": "Email templates for Manylead",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsc",
    "dev:email": "email dev",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint --flag unstable_native_nodejs_ts_config",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@react-email/button": "0.1.0",
    "@react-email/components": "0.5.7",
    "@react-email/head": "0.0.12",
    "@react-email/html": "0.0.11",
    "@react-email/render": "1.4.0",
    "@react-email/tailwind": "1.0.5",
    "@t3-oss/env-core": "catalog:",
    "react-email": "4.3.1",
    "resend": "4.6.0",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@manylead/eslint-config": "workspace:*",
    "@manylead/prettier-config": "workspace:*",
    "@manylead/tsconfig": "workspace:*",
    "@types/node": "catalog:",
    "@types/react": "catalog:react19",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "react": "19.2.0",
    "typescript": "catalog:"
  },
  "prettier": "@manylead/prettier-config"

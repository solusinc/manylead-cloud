{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "stream",
  "tasks": {
    "topo": {
      "dependsOn": ["^topo"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".cache/tsbuildinfo.json", "dist/**"]
    },
    "dev": {
      "dependsOn": ["^dev"],
      "cache": false,
      "persistent": false
    },
    "format": {
      "outputs": [".cache/.prettiercache"],
      "outputLogs": "new-only"
    },
    "lint": {
      "dependsOn": ["^topo", "^build"],
      "outputs": [".cache/.eslintcache"]
    },
    "typecheck": {
      "dependsOn": ["^topo", "^build"],
      "outputs": [".cache/tsbuildinfo.json"]
    },
    "start": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    },
    "push": {
      "cache": false,
      "interactive": true
    },
    "studio": {
      "cache": false,
      "persistent": true
    },
    "ui-add": {
      "cache": false,
      "interactive": true
    }
  },
  "globalEnv": [
    "AUTH_REDIRECT_PROXY_URL",
    "AUTH_SECRET",
    "BETTER_AUTH_URL",
    "BRIGHT_DATA_API_TOKEN",
    "DATABASE_URL",
    "DATABASE_URL_DIRECT",
    "ENCRYPTION_KEY",
    "EVOLUTION_API_KEY",
    "EVOLUTION_API_URL",
    "NEXT_PUBLIC_APP_URL",
    "NEXT_PUBLIC_SENTRY_DSN",
    "NEXT_PUBLIC_SOCKET_URL",
    "PORT",
    "POSTGRES_PASSWORD",
    "POSTGRES_USER",
    "QUEUE_TENANT_MIGRATION",
    "QUEUE_TENANT_PROVISIONING",
    "R2_ACCOUNT_ID",
    "R2_ACCESS_KEY_ID",
    "R2_SECRET_ACCESS_KEY",
    "R2_BUCKET_NAME",
    "R2_PUBLIC_URL",
    "REDIS_URL",
    "RESEND_API_KEY",
    "SENTRY_DSN",
    "SERVER_PORT",
    "SOCKET_IO_CORS_ORIGIN",
    "TENANT_MIGRATIONS_PATH",
    "WEBHOOK_BASE_URL",
    "META_WEBHOOK_VERIFY_TOKEN",
    "META_APP_SECRET",
    "META_ACCESS_TOKEN",
    "META_PHONE_NUMBER_ID",
    "META_BUSINESS_ACCOUNT_ID",
    "META_API_VERSION"
  ],
  "globalPassThroughEnv": [
    "NODE_ENV",
    "CI",
    "npm_lifecycle_event",
    "SKIP_ENV_VALIDATION",
    "ENABLE_TEST_CRON",
    "ENABLE_DRY_RUN"
  ]
}
